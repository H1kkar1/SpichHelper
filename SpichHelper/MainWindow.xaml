<Window x:Class="SpichHelper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpichHelper"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Button" HorizontalAlignment="Left" Margin="49,27,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button x:Name="Play" Content="Start" HorizontalAlignment="Left" Margin="110,27,0,0" VerticalAlignment="Top" Click="Speek_Play"/>
        <Button x:Name="Stop" Content="Stop" HorizontalAlignment="Left" Margin="110,27,0,0" VerticalAlignment="Top" Click="Speek_Close"/>

    </Grid>
</Window>
